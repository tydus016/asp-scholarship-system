@model List<WebApplication1.Models.Domain.Employee>
    
@{
}

<h1>Emplyoees lists</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Salary</th>
            <th>Date of Birth</th>
            <th>Department</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var employee in Model)
        {
            <tr>
                <td>lol++</td>
                <td>@employee.Name</td>
                <td>@employee.Email</td>
                <td>@employee.Salary</td>
                <td>@employee.DateOfBirth.ToString("dd-mm-yyyy")</td>
                <td>@employee.Department</td>
            </tr>
        }
    </tbody>
</table>
